# mount point	fstype		device

/efs		ext4		/dev/block/mmcblk0p21		flags=display="EFS";backup=1
/system		ext4		/dev/block/mmcblk0p24
/cache		ext4		/dev/block/mmcblk0p26
/data		ext4		/dev/block/mmcblk0p25

/sdcard0	auto		/dev/block/mmcblk0p28		flags=display="Internal SDcard";storage;wipeingui
/sdcard1	auto		/dev/block/mmcblk1p1		flags=display="MicroSD card";storage;wipeingui;removable

/boot		emmc		/dev/block/mmcblk0p8
/recovery	emmc		/dev/block/mmcblk0p22
